
			<extension xmlns="http://ns.adobe.com/air/extension/3.6">
				<id>com.stuv.ane.chartboost.AneChartboost</id>
				<versionNumber>1</versionNumber>

				<platforms>
		
					
		<platform name="Android-ARM">
				<applicationDeployment>
					<nativeLibrary>android.jar</nativeLibrary>
					<initializer>com.stuv.ane.chartboost.extensions.ChartboostExtension</initializer>
				</applicationDeployment>
			</platform>
		
					
		<platform name="iPhone-ARM">
				<applicationDeployment>
					<nativeLibrary>libChartboostAne.a</nativeLibrary>
					<initializer>chartboostExtensionInitializer</initializer>
					<finalizer>chartboostExtensionFinalizer</finalizer>
				</applicationDeployment>
			</platform>
		
					
		<platform name="default">
				<applicationDeployment />
			</platform>
		
		
				</platforms>
			</extension>
		