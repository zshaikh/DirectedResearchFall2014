
			<extension xmlns="http://ns.adobe.com/air/extension/3.6">
				<id>com.stuv.ane.info.AneInfo</id>
				<versionNumber>1</versionNumber>

				<platforms>
		
					
		<platform name="Android-ARM">
				<applicationDeployment>
					<nativeLibrary>android.jar</nativeLibrary>
					<initializer>com.stuv.ane.info.extensions.Extension</initializer>
				</applicationDeployment>
			</platform>
		
					
		<platform name="iPhone-ARM">
				<applicationDeployment>
					<nativeLibrary>libAneInfo.a</nativeLibrary>
					<initializer>infoExtensionInitializer</initializer>
					<finalizer>infoExtensionFinalizer</finalizer>
				</applicationDeployment>
			</platform>
		
					
		<platform name="default">
				<applicationDeployment />
			</platform>
		
		
				</platforms>
			</extension>
		